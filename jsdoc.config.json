{
  "compilerOptions": {
    "checkJs": true,
    "allowJs": true,
    "jsx": "react",
    "maxNodeModuleJsDepth": 2
  },
  "include": ["src/**/*.js", "src/**/*.jsx"],
  "exclude": ["node_modules"],
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src/Global/API/Hooks/useFetchAPI.jsx",
      "src/Global/API/fetch.instance.js",
      "src/Global/ContextProviderState.jsx",
      "src/Global/Hooks/useGlobalState.jsx",
      "src/Global/components/Modal.components.jsx",
      "src/Global/components/ModalType/ModalResult.components.jsx",
      "src/Global/function/modalStateController.js"
    ],
    "excludePattern": "(^|\\/|\\\\)node_modules"
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs",
    "readme": "./README.doc.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    },
    "docdash": {
      "static": false,
      "sort": true
    }
  }
}
